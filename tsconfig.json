{
  "compilerOptions": {
    "module": "commonjs",
    "jsx": "react",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "./",
    "paths": {
      "@components/*": ["src/components/*"]
    },
    "types": ["tailwindcss", "jest", "node"],
    "typeRoots": ["./src/types", "./node_modules/@types"]
  },
  "include": ["src"]
}
}
